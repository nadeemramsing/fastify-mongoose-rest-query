  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/server.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "outputCapture": "std"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program via npm test",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "test"
      ],
      "outputCapture": "std"
    }
  ]
}